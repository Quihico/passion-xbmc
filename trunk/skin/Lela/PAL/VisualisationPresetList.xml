<window id="122">
	<defaultcontrol>2</defaultcontrol>
	<animation effect="fade" time="400">WindowOpen</animation>
	<animation effect="fade" time="400">WindowClose</animation>

	<include>dialog-open</include>

	<coordinates>
		<system>1</system>
		<posx>80</posx>
		<posy>50</posy>
	</coordinates>

	<controls>
		<control type="image">
			<posx>-35</posx>
			<posy>-30</posy>
			<width>640</width>
			<height>556</height>
			<texture border="38">dialogpanel.png</texture>
		</control>

		<control type="label" id="3">
			<description>Presets Label</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>420</width>
			<font>h1</font>
			<textcolor>label-text</textcolor>
		</control>

		<control type="label" id="4">
			<description>No Presets Label</description>
			<posx>0</posx>
			<posy>100</posy>
			<width>408</width>
			<align>center</align>
			<label>13389</label>
			<font>h4</font>
		</control>

		<control type="listcontrol" id="2">
			<description>Preset List</description>
			<posx>0</posx>
			<posy>40</posy>
			<width>407</width>
			<height>300</height>
			<spinposx>371</spinposx>
			<spinposy>390</spinposy>
			<font>menu</font>
			<textcolor>button-text</textcolor>
			<textoffsetx>10</textoffsetx>
			<textureheight>35</textureheight>
			<onleft>2</onleft>
			<onright>2</onright>
			<onup>2</onup>
			<ondown>2</ondown>
		</control>

	</controls>
</window>